{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Replay",
    "id": "replay",
    "version": "0.5.0",
    "url": null,
    "additionalData": {
      "overrideSoName": "libreplay.so"
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1 $0?"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?",
        "pwsh ./scripts/restart-game.ps1"
      ],
      "del": [
        "pwsh ./scripts/delete.ps1",
        "pwsh ./scripts/restart-game.ps1"
      ],
      "log": [
        "pwsh ./scripts/start-logging.ps1 $0:?"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 $0?",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1 $0?"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 -analyze"
      ]
    },
    "qmodIncludeDirs": [],
    "qmodIncludeFiles": [],
    "qmodOutput": null
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.0",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "^4004.0.0",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.4.2",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.0",
      "additionalData": {}
    },
    {
      "id": "sombrero",
      "versionRange": "^0.1.40",
      "additionalData": {}
    },
    {
      "id": "conditional-dependencies",
      "versionRange": "^0.3.0",
      "additionalData": {}
    },
    {
      "id": "hollywood",
      "versionRange": "^1.1.0",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^2.0.2",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.47",
      "additionalData": {}
    },
    {
      "id": "metacore",
      "versionRange": "^1.0.0",
      "additionalData": {}
    },
    {
      "id": "songcore",
      "versionRange": "^1.1.20",
      "additionalData": {}
    }
  ]
}
